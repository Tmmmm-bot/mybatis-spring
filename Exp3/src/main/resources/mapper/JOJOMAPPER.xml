<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.exp3.mapper.JOJOMAPPER">

    <insert id="insertBatch">
        INSERT INTO jojo (id, name, introduction, sale, number)
        VALUES
        <foreach collection="list" item="i" separator=",">
            (#{i.id}, #{i.name}, #{i.introduction}, #{i.sale}, #{i.number})
        </foreach>
    </insert>

</mapper>
